{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "outDir": "temp",
        "rootDir": "src",
        "types": ["cypress"],
        "emitDeclarationOnly": true,
        "declarationDir": "temp/types",
        "skipLibCheck": true
    },
    "include": [
        "src/pageobject/**/*.ts",
        "src/**/pageobjects.ts",
        "src/**/*.pageobject.ts"
    ],
    "exclude": ["src/pageobject/**/*cy.ts"]
}
