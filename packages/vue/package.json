{
  "name": "@fkui/vue",
  "version": "6.6.0",
  "description": "Vue implementation of FKUI components",
  "keywords": [
    "fkui",
    "designsystem",
    "vue"
  ],
  "homepage": "https://designsystem.forsakringskassan.se/",
  "bugs": "https://github.com/Forsakringskassan/designsystem/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/designsystem.git",
    "directory": "packages/vue"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs.js",
      "import": "./dist/esm/index.esm.js"
    },
    "./cypress": {
      "types": "./dist/types/cypress.d.ts",
      "require": "./dist/cjs/cypress.cjs.js",
      "import": "./dist/esm/cypress.esm.js"
    },
    "./htmlvalidate": "./htmlvalidate/index.cjs",
    "./htmlvalidate/cypress": "./htmlvalidate/cypress.js",
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig-consumer.json"
  },
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/esm/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "htmlvalidate",
    "tsconfig-consumer.json",
    "!**/*.spec.{js,ts,d.ts}",
    "!**/.eslintrc.json",
    "!htmlvalidate/tsconfig.json"
  ],
  "scripts": {
    "prebuild": "run-s clean",
    "build": "run-s build:lib build:pageobjects build:dts build:api",
    "build:api": "fk-api-extractor --patch-augmentations api-extractor.*.json",
    "build:dts": "vue-tsc -b",
    "build:lib": "fk-build-vue-lib",
    "build:pageobjects": "node build-pageobjects.mjs",
    "clean": "rimraf -g .jest-cache *.tsbuildinfo coverage dist public temp test-results",
    "prepack": "release-prepack --bundle --retain-scripts",
    "postpack": "release-postpack",
    "prepublishOnly": "release-prepublish --bundle --retain-scripts",
    "start": "vite --",
    "test": "jest --coverage",
    "unit": "jest",
    "unit:watch": "jest --watch"
  },
  "jest": {
    "globals": {
      "vue-jest": {
        "compilerOptions": {
          "comments": false
        }
      }
    },
    "preset": "@forsakringskassan/jest-config-vue",
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^(.+\\.css)\\?raw$": "$1"
    }
  },
  "dependencies": {
    "@vueuse/core": "12.8.2",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@fkui/date": "6.6.0",
    "@fkui/design": "6.6.0",
    "@fkui/icon-lib-default": "6.6.0",
    "@fkui/logic": "6.6.0",
    "@fkui/test-utils": "6.6.0",
    "@fkui/theme-default": "6.6.0",
    "@forsakringskassan/vite-lib-config": "3.6.4",
    "@vue/test-utils": "2.4.6",
    "cypress": "14.3.2",
    "esbuild": "0.25.3",
    "flush-promises": "1.0.2",
    "vite": "6.3.3",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "peerDependencies": {
    "@fkui/date": "^6.6.0",
    "@fkui/design": "^6.6.0",
    "@fkui/logic": "^6.6.0",
    "fk-icons": "^4.30.1",
    "html-validate": ">= 7.9.0",
    "vue": "^3.5.0"
  },
  "peerDependenciesMeta": {
    "fk-icons": {
      "optional": true
    },
    "html-validate": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 7"
  },
  "externalDependencies": []
}
