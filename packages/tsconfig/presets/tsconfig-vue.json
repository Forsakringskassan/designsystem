{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "@fkui/tsconfig/vue",

    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "bundler",
        "emitDeclarationOnly": true,
        "skipLibCheck": true
    },

    "vueCompilerOptions": {
        "experimentalModelPropName": {
            "": {
                "f-radio-field": true,
                "f-checkbox-field": true
            }
        }
    }
}
