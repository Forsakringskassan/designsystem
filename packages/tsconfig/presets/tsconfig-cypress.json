{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "@fkui/tsconfig/cypress",

    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "bundler",
        "noEmit": true,
        "resolveJsonModule": true,
        "strict": true,
        "target": "esnext",
        "lib": ["es6", "dom"],
        "types": ["cypress", "node"]
    }
}
