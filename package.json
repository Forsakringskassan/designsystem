{
  "name": "@fkui/fkui-monorepo",
  "version": "6.6.0",
  "private": true,
  "description": "fkui monorepo",
  "homepage": "https://designsystem.forsakringskassan.se/",
  "bugs": "https://github.com/Forsakringskassan/designsystem/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/designsystem.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "main": "index.js",
  "files": [],
  "workspaces": [
    "docs",
    "packages/*",
    "internal/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "build:docs": "lerna run build:docs && node generate-docs.mjs && run-s --continue-on-error build:docs:*",
    "build:docs:html-validate": "html-validate --formatter stylish,codeframe --config .htmlvalidate-public.js public",
    "build:docs:tsc": "tsc -b docs/examples",
    "build:docs:eslint": "eslint --cache docs/examples",
    "build:docs:sass": "node scripts/example-sass.mjs docs/examples/**/*.scss",
    "build:docs:stylelint": "stylelint docs/examples/**/*.{css,scss}",
    "preclean": "rimraf -g .eslintcache *.tgz public temp test-results",
    "clean": "npm run --workspaces --if-present clean",
    "theme-default": "npm run --workspace packages/theme-default --",
    "cypress": "cypress",
    "date": "npm run --workspace packages/date --",
    "design": "npm run --workspace packages/design --",
    "eslint": "eslint --cache .",
    "eslint:fix": "eslint --fix .",
    "html-validate": "html-validate --formatter stylish,codeframe --ext md,html,vue docs packages/*/src internal/*/src",
    "labs": "npm run --workspace packages/vue-labs --",
    "lerna": "lerna",
    "link-all": "lerna exec npm link",
    "lint": "run-s prettier:check eslint stylelint html-validate",
    "lint:fix": "run-s prettier:write eslint:fix stylelint:fix",
    "logic": "npm run --workspace packages/logic --",
    "prepare": "husky",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "sandbox": "npm run --workspace internal/vue-sandbox --",
    "start": "fkui-serve public temp/docs",
    "start:cypress": "node cypress/server.js",
    "start:demo": "run-s build:docs start",
    "start:sandbox": "run-s build:docs && fkui-serve public/vue-sandbox",
    "stylelint": "stylelint \"packages/*/src/**/*.{css,scss}\" \"internal/*/src/**/*.{css,scss}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "pretest": "npm run lint",
    "test": "lerna run test",
    "test:ct": "cypress run-ct --headless",
    "vue": "npm run --workspace packages/vue --"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config"
  },
  "lint-staged": {
    "*.{json,md,css,scss,html}": "prettier --write",
    "*.{js,cjs,mjs,vue,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "prettier": "@forsakringskassan/prettier-config",
  "stylelint": {
    "extends": [
      "@forsakringskassan/stylelint-config",
      "./packages/design/stylelint/recommended"
    ],
    "rules": {
      "alpha-value-notation": null,
      "color-function-notation": null,
      "media-feature-range-notation": null
    }
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/preset-env": "7.26.9",
    "@forsakringskassan/commitlint-config": "1.4.2",
    "@forsakringskassan/cypress-axe": "3.2.1",
    "@forsakringskassan/cypress-visual-regression": "2.4.0",
    "@forsakringskassan/docs-generator": "2.26.3",
    "@forsakringskassan/docs-live-example": "2.0.0",
    "@forsakringskassan/eslint-config": "11.9.3",
    "@forsakringskassan/eslint-config-cli": "11.9.0",
    "@forsakringskassan/eslint-config-cypress": "11.9.0",
    "@forsakringskassan/eslint-config-jest": "11.9.3",
    "@forsakringskassan/eslint-config-typescript": "11.9.3",
    "@forsakringskassan/eslint-config-vue": "11.9.3",
    "@forsakringskassan/jest-config": "29.6.2",
    "@forsakringskassan/jest-config-vue": "29.6.2",
    "@forsakringskassan/prettier-config": "3.0.5",
    "@forsakringskassan/sass-module-importer": "1.1.4",
    "@forsakringskassan/stylelint-config": "2.5.0",
    "@forsakringskassan/vite-lib-config": "3.6.4",
    "@html-validate/release-scripts": "6.8.0",
    "@types/estree": "1.0.7",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/node": "20.17.30",
    "@vue/server-renderer": "3.5.13",
    "autoprefixer": "10.4.21",
    "cli-table": "0.3.11",
    "core-js": "3.41.0",
    "cypress": "14.3.2",
    "cypress-html-validate": "7.1.0",
    "esbuild": "0.25.3",
    "flush-promises": "1.0.2",
    "fs-extra": "11.3.0",
    "glob": "11.0.2",
    "html-validate": "9.5.3",
    "html-validate-markdown": "5.0.0",
    "html-validate-vue": "7.1.5",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "lerna": "8.2.2",
    "lint-staged": "15.5.1",
    "mocha-junit-reporter": "2.2.1",
    "mocha-multi-reporters": "1.5.1",
    "npm-pkg-lint": "3.10.11",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.3",
    "rimraf": "6.0.1",
    "sass": "1.87.0",
    "typedoc": "0.28.3",
    "typescript": "5.8.3",
    "vite": "6.3.3",
    "vue": "3.5.13",
    "vue-tsc": "2.2.0"
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 7"
  }
}
